package ${packageName};

import com.mybatisflex.core.query.QueryColumn;
import com.mybatisflex.core.table.TableDef;

class ${className} : TableDef("", "${talbeName}") {

    #foreach($column in $list)
        val $column.name = QueryColumn(this, "${column.columnName}")
    #end
    /**
     * 所有字段。
     */
    val ALL_COLUMNS = QueryColumn(this, "*")

    /**
     * 默认字段，不包含逻辑删除或者 large 等字段。
     */
    val DEFAULT_COLUMNS = arrayOf(#foreach($column in $list) $column.name #if($foreach.hasNext),#end#end)


    companion object {
        val  $instance = ${className}()
    }
}
